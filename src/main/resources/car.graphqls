type Car {
    make: String!
    model: String!
    engine: Engine!
}

type Engine {
    id: Int!
    capacity: Int! @deprecated
    fuel: String!
}

type Query {
    engines: [Engine]
    car(make: String): Car
    cars: [Car]
}

type Mutation {
    updateEngine(id: Int!, capacity: Int!): Engine
}

schema {
    query: Query
    mutation: Mutation
}
